{
    "name": "fitnexp-backend",
    "version": "1.0.0",
    "description": "This is the repository for the backend source code of Fitnexp",
    "main": "src/index.ts",
    "scripts": {
        "build": "tsc",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "start": "node dist/src/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint .",
        "populate": "tsc && node dist/src/populate/populateScript.js"
    },
    "author": "José Miguel García Casillas",
    "license": "UNLICENSED",
    "dependencies": {
        "bcrypt": "5.1.1",
        "cookie-parser": "1.4.6",
        "cors": "2.8.5",
        "dotenv": "16.4.5",
        "express": "4.19.2",
        "jsonwebtoken": "9.0.2",
        "mongoose": "8.5.1",
        "swagger-jsdoc": "6.2.8",
        "swagger-ui-express": "5.0.1"
    },
    "devDependencies": {
        "@eslint/js": "9.6.0",
        "@semantic-release/git": "10.0.1",
        "@semantic-release/github": "10.1.3",
        "@types/bcrypt": "5.0.2",
        "@types/cookie-parser": "1.4.7",
        "@types/cors": "2.8.17",
        "@types/eslint__js": "8.42.3",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.12",
        "@types/jsonwebtoken": "9.0.6",
        "@types/supertest": "6.0.2",
        "@types/swagger-jsdoc": "6.0.4",
        "@types/swagger-ui-express": "4.1.6",
        "eslint": "8.56.0",
        "globals": "15.8.0",
        "jest": "29.7.0",
        "prettier": "3.3.2",
        "semantic-release": "24.0.0",
        "supertest": "7.0.0",
        "ts-jest": "29.2.3",
        "ts-node-dev": "2.0.0",
        "typescript": "5.5.3",
        "typescript-eslint": "7.16.0"
    }
}
